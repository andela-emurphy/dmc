{
  "name": "document-management-system",
  "version": "1.0.0",
  "description": "The system manages documents, users and user roles. Each document defines access rights; the document defines which roles can access it.",
  "main": "server.js",
  "scripts": {
    "start": "nodemon --exec npm run babel-node",
    "babel-node": "node_modules/.bin/babel-node  server.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "node_modules/.bin/eslint app/** test/** ",
    "seed": "node_modules/.bin/sequelize db:seed:all",
    "seed:undo": "node_modules/.bin/sequelize db:seed:undo:all",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "migrate:undo": "node_modules/.bin/sequelize db:migrate:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-emurphy/document-management-system.git"
  },
  "author": "Enaho Murphy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela-emurphy/document-management-system/issues"
  },
  "homepage": "https://github.com/andela-emurphy/document-management-system#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-plugin-transform-es2015-classes": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-watch": "^2.0.5",
    "coveralls": "^2.11.15",
    "eslint": "^3.13.0",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.2.0",
    "nyc": "^10.0.0"
  },
  "dependencies": {
    "Faker": "^0.7.2",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-preset-node5": "^12.0.0",
    "bcryptjs": "^2.4.0",
    "body-parser": "^1.16.0",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "express-jwt": "^5.1.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-sourcemaps": "^2.4.0",
    "jsonwebtoken": "^7.2.1",
    "moment": "^2.17.1",
    "morgan": "^1.8.1",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.28.0",
    "sequelize-cli": "^2.5.1",
    "sqlite3": "^3.1.8",
    "underscore": "^1.8.3"
  }
