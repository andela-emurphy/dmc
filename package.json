{
  "name": "document-management-system",
  "version": "1.0.0",
  "description": "The system manages documents, users and user roles. Each document defines access rights; the document defines which roles can access it.",
  "main": "es5/server.js",
  "scripts": {
    "start": "node es5/server.js",
    "production": "node_modules/.bin/gulp babel && npm start",
    "babel-node": "nodemon --exec node_modules/.bin/babel-node  server/server.js",
    "build": "babel server.js -d src",
    "coveralls": "npm run coverage -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "lint": "node_modules/.bin/eslint app/** test/** ",
    "seed": "npm run migrate && node_modules/.bin/sequelize db:seed:all",
    "seed:undo": "node_modules/.bin/sequelize db:seed:undo:all",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "migrate:undo": "node_modules/.bin/sequelize db:migrate:undo:all",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/_mocha  ./test/server/**/*.js -- --require babel-register",
    "coverage": "cross-env NODE_ENV=test node_modules/.bin/istanbul cover  ./node_modules/.bin/_mocha  ./test/server/**/*.js -- --require babel-register"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "reporter": [
    "text-lcov",
    "text-summary",
    "html"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-emurphy/document-management-system.git"
  },
  "author": "Enaho Murphy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela-emurphy/document-management-system/issues"
  },
  "homepage": "https://github.com/andela-emurphy/document-management-system#readme",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.21.0",
    "babel-istanbul": "^0.12.1",
    "babel-plugin-istanbul": "^3.1.2",
    "babel-register": "^6.23.0",
    "babel-watch": "^2.0.5",
    "bluebird": "^3.4.7",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "coveralls": "^2.11.16",
    "cross-env": "^3.1.4",
    "eslint": "^3.13.0",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-mocha": "^3.0.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-sourcemaps": "^2.4.0",
    "istanbul": "^1.1.0-alpha.1",
    "istanbul-harmony": "^0.3.16",
    "mocha": "^3.2.0",
    "node-mocks-http": "^1.6.1",
    "nyc": "^10.0.0",
    "promisify-supertest": "^1.0.0",
    "sinon": "^1.17.7",
    "supertest": "^3.0.0",
    "transform-runtime": "0.0.0"
  },
  "dependencies": {
    "Faker": "^0.7.2",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-node5": "^12.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.23.0",
    "bcryptjs": "^2.4.0",
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.0",
    "chai-as-promised": "^6.0.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "cors": "^2.8.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "express-jwt": "^5.1.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-mocha": "^3.0.1",
    "gulp-nodemon": "^2.2.1",
    "istanbul": "^0.4.5",
    "jsonwebtoken": "^7.2.1",
    "moment": "^2.17.1",
    "morgan": "^1.8.1",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.28.0",
    "sequelize-cli": "^2.5.1",
    "should": "^11.2.0",
    "sqlite3": "^3.1.8",
    "underscore": "^1.8.3"
  }
}
